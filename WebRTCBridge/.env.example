# WebRTC Bridge Configuration
# Copy this file to .env and fill in your actual values

# Livepeer Configuration
LIVEPEER_API_KEY=your-livepeer-api-key-here
LIVEPEER_STREAM_KEY=your-stream-key-here
LIVEPEER_STREAM_ID=your-stream-id-here
LIVEPEER_PLAYBACK_ID=your-playback-id-here

# RTMP Configuration
RTMP_INGEST_URL=rtmp://rtmp.livepeer.com/live

# SRT Configuration
SRT_INGEST_URL=srt://rtmp.livepeer.com:2935

# Playback URLs
HLS_PLAYBACK_URL=https://livepeercdn.studio/hls/YOUR_PLAYBACK_ID/index.m3u8